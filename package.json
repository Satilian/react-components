{
  "name": "package-based",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {},
  "private": true,
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.9.2",
    "nx": "15.9.2",
    "typescript": "^5.0.4"
  },
  "workspaces": ["packages/*"],
  "nx": {
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "npmScope": "package-based",
    "affected": {
      "defaultBase": "master"
    },
    "tasksRunnerOptions": {
      "default": {
        "runner": "nx/tasks-runners/default",
        "options": {
          "cacheableOperations": [
            "build",
            "lint",
            "test",
            "e2e"
          ]
        }
      }
    },
    "targetDefaults": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "production",
          "^production"
        ]
      }
    },
    "namedInputs": {
      "default": [
        "{projectRoot}/**/*",
        "sharedGlobals"
      ],
      "production": [
        "default"
      ],
      "sharedGlobals": []
    }
  }
}
